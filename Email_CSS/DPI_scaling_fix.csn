<?xml version="1.0" encoding="utf-8"?>
<snippet name="DPI scaling fix" description="Fixes images in Outlook when using DPI scaling on windows" preview="code" type="block">
<insertText location="beforeSelection">
<![CDATA[<!--[if gte mso 9]>
<xml>
    <o:OfficeDocumentSettings>
        <o:AllowPNG/>
        <o:PixelsPerInch>96</o:PixelsPerInch>
    </o:OfficeDocumentSettings>
</xml>
<![endif]-->]]>
</insertText>
<insertText location="afterSelection"><![CDATA[]]>
</insertText>
</snippet>
